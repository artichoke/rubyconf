{
  "name": "@artichoke/rubyconf",
  "version": "0.2.0",
  "private": true,
  "description": "Artichoke Ruby conference talk website",
  "keywords": [
    "programming language",
    "scripting",
    "ruby",
    "rust"
  ],
  "homepage": "https://artichoke.github.io/rubyconf/",
  "bugs": "https://github.com/artichoke/rubyconf/issues",
  "license": "MIT",
  "author": "Ryan Lopopolo <rjl@hyperbo.la> (https://hyperbo.la/)",
  "contributors": [
    "Ryan Lopopolo <rjl@hyperbo.la> (https://hyperbo.la/)"
  ],
  "repository": "github:artichoke/rubyconf",
  "dependencies": {
    "jquery": "^3.5.1",
    "reveal.js": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-html": "^6.0.3",
    "eslint-plugin-import": "^2.22.1",
    "file-loader": "^6.1.0",
    "html-loader": "^1.3.1",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^0.11.2",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "raw-loader": "^4.0.1",
    "style-loader": "^1.2.1",
    "svgo": "^1.3.0",
    "svgo-loader": "^2.2.1",
    "terser-webpack-plugin": "^4.2.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "plugins": [
      "html",
      "import"
    ],
    "extends": [
      "eslint:recommended"
    ],
    "settings": {
      "import/resolver": "webpack"
    }
  },
  "scripts": {
    "dev": "npx webpack-dev-server",
    "dev:open": "npx webpack-dev-server --open",
    "fmt": "npx prettier --write '**/*'",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint --fix ."
  }
}
