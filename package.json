{
  "name": "artichoke-playground",
  "version": "0.1.0",
  "private": true,
  "description": "Artichoke Ruby conference talk website",
  "keywords": [
    "programming language",
    "scripting",
    "ruby",
    "rust"
  ],
  "homepage": "https://artichoke.github.io/rubyconf/",
  "bugs": "https://github.com/artichoke/rubyconf/issues",
  "license": "MIT",
  "author": "Ryan Lopopolo <rjl@hyperbo.la> (https://hyperbo.la/)",
  "contributors": [
    "Ryan Lopopolo <rjl@hyperbo.la> (https://hyperbo.la/)"
  ],
  "repository": "github:artichoke/rubyconf",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.1.0",
    "eslint": "^5.11.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.7.0",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "raw-loader": "^3.1.0",
    "style-loader": "^0.23.1",
    "svgo": "^1.3.0",
    "svgo-loader": "^2.2.1",
    "terser-webpack-plugin": "^1.3.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "plugins": [
      "html",
      "import"
    ],
    "extends": [
      "eslint:recommended"
    ],
    "settings": {
      "import/resolver": "webpack"
    }
  },
  "dependencies": {
    "highlight.js": "^9.15.10",
    "reveal.js": "^3.8.0",
    "reveal_external": "^1.3.0",
    "reveald3": "^1.5.5"
  },
  "scripts": {
    "dev": "npx webpack-dev-server",
    "dev:open": "npx webpack-dev-server --open",
    "fmt": "npx prettier --write '**/*'",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint --fix ."
  }
}
